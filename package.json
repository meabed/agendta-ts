{
  "name": "agenda-ts",
  "version": "1.6.9-beta.0",
  "description": "The modern MongoDB-powered job scheduler library for Node.js",
  "keywords": [
    "job",
    "jobs",
    "cron",
    "cronjob",
    "mongodb",
    "scheduling",
    "mongodb-scheduler",
    "queue",
    "delayed",
    "scheduler",
    "pulse",
    "bull",
    "bee",
    "kue"
  ],
  "homepage": "https://github.com/meabed/agenda-ts",
  "bugs": {
    "url": "https://github.com/meabed/agenda-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meabed/agenda-ts.git"
  },
  "license": "MIT",
  "author": "Mohamed Meabed <mo.meabed@gmail.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dry-run": "npm publish --dry-run",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "prepublishOnly": "npm run build",
    "release": "npm run build && semantic-release",
    "semantic-release": "semantic-release",
    "test": "jest --detectOpenHandles --forceExit",
    "test:coverage": "jest --coverage",
    "test:debug": "DEBUG=pulse:** jest"
  },
  "config": {
    "blanket": {
      "pattern": "src",
      "data-cover-never": "node_modules"
    }
  },
  "dependencies": {
    "cron-parser": "^5.4.0",
    "date.js": "~0.3.3",
    "debug": "~4.4.3",
    "human-interval": "~2.0.1",
    "moment-timezone": "^0.6.0",
    "mongodb": "^7.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/debug": "^4.1.12",
    "@types/human-interval": "^1.0.2",
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "delay": "7.0.0",
    "eslint": "^9.39.2",
    "jest": "30.2.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.7.4",
    "semantic-release": "^25.0.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@1.22.22",
  "publishConfig": {
    "access": "public"
  }
}
